@media (max-width: 576px)
  .chat-list-page, .chat-wrapper-page
    width 100%
    height 100%
  .chat-wrapper-page
    margin-left 0
    border-right none
    height 100vh
    z-index 2
  .chat-list-page
    -webkit-overflow-scrolling touch
  .fiexed-page
    width 100%
    margin-left 0px
@media (min-width: 576px) and (max-width: 767px)
  @import "./sm"
@media (min-width: 768px) and (max-width: 991px)
  @import "./md"
@media (min-width: 992px) and (max-width: 1199px)
  @import "./lg"
@media (min-width: 1200px)
  @import "./elg"

