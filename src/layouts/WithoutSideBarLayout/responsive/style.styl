@media (max-width: 576px)
  .chat-list-without, .chat-wrapper-without
    width 100%
    height 100%
  .chat-wrapper-without
    margin-left 0
    border-right none
    position fixed
    top 0
    left 0
    height 100%
    z-index 2
  .chat-list-without
    -webkit-overflow-scrolling touch
@media (min-width: 576px) and (max-width: 767px)
  @import "./sm"
@media (min-width: 768px) and (max-width: 991px)
  @import "./md"
@media (min-width: 992px) and (max-width: 1199px)
  @import "./lg"
@media (min-width: 1200px)
  @import "./elg"

